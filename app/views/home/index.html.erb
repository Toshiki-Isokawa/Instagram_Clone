<div>
    <% if user_signed_in? %>
        <%= render 'posts/posts', posts: @posts %>
        <div>
            <%= link_to '投稿', new_post_path %>
        </div>
    <% else %>
        <%= link_to 'サインアップ', new_user_registration_path %>
        <%= link_to 'ログイン', new_user_session_path %>
    <% end %>
</div>